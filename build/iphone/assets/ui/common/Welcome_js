function Welcome(){var i=("android"===Ti.Platform.name?16:14,Ti.Platform.displayCaps.platformWidth,Ti.Platform.displayCaps.platformHeight,Ti.Platform.osname);Ti.UI.setBackgroundColor("#fff");var e=Ti.UI.createWindow({backgroundColor:"white",exitOnClose:!1,fullscreen:!1,modal:!0,navBarHidden:!0,title:"Radar Food Truck",opacity:1,width:"100%",height:"100%"}),a=Ti.UI.createView({width:"100%",height:"100%",backgroundImage:"/ui/img/SALUDO/bg.png"}),t=Ti.UI.createImageView({image:"/ui/img/SALUDO/2.png",width:"55%",height:"75%"}),o=Ti.UI.createImageView({image:"/ui/img/SALUDO/banderin.png",top:"0%",left:"5%",width:"15%",height:"15%",zIndex:9999});return e.addEventListener("click",function(){if("iphone"===i||"ipad"===i)var e=require("ui/common/IndexIOS");else var e=require("ui/common/Index");(new e).open()}),e.add(a),e.add(t),e.add(o),e}module.exports=Welcome;